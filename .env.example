# CDN Configuration
PUBLIC_CDN_URL=http://localhost:5173

# Webhook Configuration (for N8n integration)
WEBHOOK_JWT_SECRET=your-secret-jwt-key-here
N8N_CONTACT_WEBHOOK_URL=https://your-n8n-instance.com/webhook/contact-form
N8N_HEALTH_CHECK_WEBHOOK_URL=https://your-n8n-instance.com/webhook/health-check-form

# Rate Limiting
RATE_LIMIT_WINDOW=60000 # 1 minute in milliseconds
RATE_LIMIT_MAX_REQUESTS=10 # Max requests per window

# Optional: Basic Auth Alternative (if not using JWT)
# WEBHOOK_AUTH_USERNAME=your-username
# WEBHOOK_AUTH_PASSWORD=your-password

# Optional: Header Auth Alternative (if not using JWT)
# WEBHOOK_AUTH_HEADER_NAME=X-Webhook-Secret
# WEBHOOK_AUTH_HEADER_VALUE=your-secret-value

# Tawk.to Configuration
PUBLIC_TAWK_PROPERTY_ID=your-tawk-property-id
PUBLIC_TAWK_WIDGET_ID=your-tawk-widget-id

# Feature Toggles
